  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.3.1",
    "@graphprotocol/graph-ts": "^0.0.1",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "eth-ens-namehash": "^2.0.8",
    "web3": "^1.0.0-beta.36"
  }
